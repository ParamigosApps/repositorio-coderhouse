{
  "version": 3,
  "name": "appbares",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    }
  ],
  "outputDirectory": "public",
  "env": {
    "MP_ACCESS_TOKEN": "@mp_access_token"
  }
}
