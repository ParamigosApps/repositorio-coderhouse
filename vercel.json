{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "appbares",
  "outputDirectory": "Assets",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1.js"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "env": {
    "MP_ACCESS_TOKEN": "@mp_access_token"
  }
}
